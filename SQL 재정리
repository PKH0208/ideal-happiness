-- 커뮤니티 게시판 -- 

-- Create the table
CREATE TABLE Board (
    id NUMBER GENERATED ALWAYS AS IDENTITY,
    id NUMBER PRIMARY KEY,
    category VARCHAR2(50),
    title VARCHAR2(255),
    content VARCHAR2(255),
    png VARCHAR2(255),
    details VARCHAR2(255),
    views INT DEFAULT 0,
    PRIMARY KEY (id),
  
);

