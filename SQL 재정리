-- 호텔&뷰티&병원&여행&음식점 -- 
CREATE TABLE active (
  
  active_id INT GENERATED ALWAYS AS IDENTITY PRIMARY KEY,  --serial Num
    con INT, --   종류 ,1:hotel, 2:beauty, 3:hospital, 4:travel , 5:restaurant 
    name VARCHAR(255),  -- 업체명
    details VARCHAR(225), -- 주차가능, 와이파이, 24시간 방문 가능, 유기농 간식 제공
    location1 VARCHAR(255), -- 서울
    location2 VARCHAR(255), -- 강남
    location3 VARCHAR(255), -- 상세 주소
    open_time VARCHAR(255), -- 오픈시간
    close_time VARCHAR(255), -- 마감시간
    TEL_NO VARCHAR(255)
);

